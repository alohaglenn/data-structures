{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ]
  }
}
